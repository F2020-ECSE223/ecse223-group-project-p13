{
  isA Account;
}

class Appointment
{
  1 appointment -- 1 TimeSlot timeSlot;
  * -> * Service currentService;
  * appointments -- * Service Services;
}

class TimeSlot
{
  Time startTime;
  Time endTime;
}

class Calendar
{
  Date date;
  1 calendar -- * Day days;
  * calendar -- * TimeSlot schedule;
}

class Service
{
  name;
  Integer duration;
  
}

class Combo
{
  
    1 serviceCombo -- * Service services;
}

class Business
{
  name;
  address;
  phoneNumber;
  emailAddress;
  1 business -- 1 DailyHour timeTable;
  1 business -- 1 OwnerAccount owner;
}

class Day
{
  Boolean isClosed;
}

class DailyHour
{
   Time openTime;
  Time endTime;
  Time lunchBreakStart;
  Time lunchBreakEnd;
  1 availability -- * Day day;
}
class CustomerAccount
{
  isA Account;
  1 <@>- * Appointment;
  0..1 account -> * Appointment currentAppointment;
}

class MainService
{
  isA Service;
}

class SubService
{
  isA Service;
}

